{
  "file": [{
    "name": "nested include",
    "template": {
      "Fn::Include": "includes/nested.json"
    },
    "output": {
      "foo": "bar",
      "More": { "foo": "bar"}
    }
  }, {
    "template": {
      "Fn::Include": "includes/foobar.json"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "name": "don't overwrite existing elements",
    "template": {
      "foo": "baz",
      "Fn::Include": "includes/foobar.json"
    },
    "output": {
      "foo": "baz"
    }
  }, {
    "template": {
      "Version": "2009-09-09",
      "Mappings": {
        "IPRanges": {
          "Fn::Include": "includes/foobar.json"
        },
        "AMIs": {
          "Fn::Include": "includes/foobar.json"
        }
      }
    },
    "output": {
      "Version": "2009-09-09",
      "Mappings": {
        "IPRanges": {
          "foo": "bar"
        },
        "AMIs": {
          "foo": "bar"
        }
      }
    }
  }, {
    "template": {
      "Version": "2009-09-09",
      "Mappings": {
        "IPRanges": [{
          "something": "else"
        }, {
          "Fn::Include": "includes/foobar.json"
        }]
      }
    },
    "output": {
      "Version": "2009-09-09",
      "Mappings": {
        "IPRanges": [{
          "something": "else"
        }, {
          "foo": "bar"
        }]
      }
    }
  }, {
    "template": {
      "Fn::Include": "includes/deep.json"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "template": {
      "Fn::Include": "../t/includes/yaml/deep.yml"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "name": "test some comments",
    "template": {
      "Fn::Include": "includes/comment.json"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "template": {
      "Fn::Include": "includes/verydeep.json"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "template": {
      "Fn::Include": "includes/subfolder.json"
    },
    "output": {
      "Mappings": {
        "Mapping1": {
          "foo": "bar"
        },
        "Mapping2": {
          "foo": "bar"
        }
      }
    }
  }, {
    "name": "include empty file",
    "template": {
      "Mappings": {
        "Fn::Include": "includes/empty.txt"
      }
    },
    "output": {
      "Mappings": null
    }
  }],
  "http": [{
    "template": {
      "Fn::Include": "https://s3.amazonaws.com/cfn-include-test/v0.0.1/includes/foobar.json"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "template": {
      "Fn::Include": "https://s3.amazonaws.com/cfn-include-test/v0.0.1/includes/deep.json"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "template": {
      "Fn::Include": "https://s3.amazonaws.com/cfn-include-test/v0.0.1/includes/verydeep.json"
    },
    "output": {
      "foo": "bar"
    }
  }, {
    "template": {
      "Fn::Include": "https://s3.amazonaws.com/cfn-include-test/v0.0.1/includes/subfolder.json"
    },
    "output": {
      "Mappings": {
        "Mapping1": {
          "foo": "bar"
        },
        "Mapping2": {
          "foo": "bar"
        }
      }
    }
  }]
}
